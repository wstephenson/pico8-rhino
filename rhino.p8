pico-8 cartridge // http://www.pico-8.com
version 4
__lua__
--globals
plx=7
ply=7
oplx=0
oply=0
den=0.08

function _init()
  cls()
  map(0,0,0,0,16,16)
end

function clamp(val,minv,maxv)
  return max(minv,min(val,maxv))
end  

function _update()
	 if (btnp(0)) then nplx=plx-1 end
	 if (btnp(1)) then nplx=plx+1 end
	 if (btnp(2)) then nply=ply-1 end
	 if (btnp(3)) then nply=ply+1 end
  nplx = clamp(nplx, 0, 15)
  nply = clamp(nply, 0, 15)

  if (mget(nplx,nply) == 0) then
    oplx = plx
    oply = ply
    plx = nplx
    ply = nply
  end
end

function _draw()
  --debug coords
  rectfill(0,0,63,8,0)
  print(plx..","..ply,0,0,3)
  --player
  rectfill(oplx*8,oply*8,oplx*8+7,oply*8+7,0)
  spr(2, plx*8, ply*8)
end
