pico-8 cartridge // http://www.pico-8.com
version 4
__lua__
--globals
p={}
p.x=0
p.y=0
p.s=true
p.nx=p.x
p.ny=p.y
r={}
r.x=0
r.y=0
wins=0
losses=0
--sprites
smpty=0
swall=8
sxit=16
p.spr=1
r.spr=24

--map
mrx=16
mry=0
den=0.3

-- general
function clamp(val,minv,maxv)
  return max(minv,min(val,maxv))
end

-- map
function mkmap()
  local mspr = 0
  for i=0,15 do
    for j=0,15 do
      if rnd(1)<den then
        mspr=swall
      else
        mspr=smpty
      end
      mset(i+mrx,j+mry,mspr)
    end
  end
  plcexit()
end

function getfreecoords()
  local x,y
  local gdmv=false
  while(not gdmv) do
    x=flr(rnd(15))
    y=flr(rnd(15))
    if(mget(mrx+x,mry+y)==0) gdmv=true
  end
  return x,y
end

function plcplyr()
  p.x,p.y=getfreecoords()  
  p.nx=p.x
  p.ny=p.y
end

function plcexit()
  local x,y=getfreecoords()
  mset(mrx+x,mry+y,sxit)
end

function plcrhino()
  r.x,r.y=getfreecoords()
end

-- game logic
function win()
  wins=wins+1
  mkmap()
  reset()
end

function lose()
  losses=losses+1
  reset()
end

function reset()
  plcplyr()
  plcrhino()
end

-- movement
function checkmv(x,y)
  local nspr=mget(mrx+x,mry+y)
  return not fget(nspr,0)
end

function haswon()
  local nspr=mget(mrx+p.x,mry+p.y)
  return fget(nspr,3)
end
  
function movep()
	 if (btnp(0)) then p.nx=p.x-1 p.ny=p.y end
	 if (btnp(1)) then p.nx=p.x+1 p.ny=p.y end
	 if (btnp(2)) then p.ny=p.y-1 p.nx=p.x end
	 if (btnp(3)) then p.ny=p.y+1 p.nx=p.x end
  p.nx=clamp(p.nx, 0, 15)
  p.ny=clamp(p.ny, 0, 15)

  if (checkmv(p.nx,p.ny) and (p.nx~=p.x or p.ny~=p.y)) then
    p.x=p.nx
    p.y=p.ny
    p.s=not p.s
    mover()
  end
  if (haswon()) then
    win()
  end
end  

function mover()
  local dx=p.x-r.x
  if(dx>0) then dx=dx/dx end
  if(dx<0) then dx=dx/-dx end
  local dy=p.y-r.y
  if(dy>0) then dy=dy/dy end
  if(dy<0) then dy=dy/-dy end
  if(checkmv(r.x+dx,r.y+dy)) then
    r.x=r.x+dx
    r.y=r.y+dy
    if(r.x==p.x and r.y==p.y) then lose() end
  end
end

-- pico8 callbacks
function _init()
  cls()
  mkmap()
  reset()
end

function _update()
  movep()
end

function _draw()
  cls()
  --background
  map(mrx,mry,0,0,16,16)
  --player
  if(p.s) then p.spr=1 else p.spr=2 end
  spr(p.spr, p.x*8, p.y*8)
  --rhino
  spr(r.spr,r.x*8,r.y*8)
  --debug coords
  print(p.x..","..p.y..":"..wins.."-"..losses,0,0,10)
end
