pico-8 cartridge // http://www.pico-8.com
version 4
__lua__
--globals
attract=true
p={}
p.x=0
p.y=0
p.s=true
r={}
r.x=0
r.y=0
r.candmv=true
r.canchg=true
r.bored=0
r.brlim=5
r.chgvc=-1
r.chgdz=2
wins=0
losses=0
--sprites
smpty=0
swall=8
sxit=16
p.spr=1
rnorm=24
rangry=25
r.spr=rnorm
--map
mrx=16
mry=0
den=0.3

-- general
function clamp(val,minv,maxv)
  return max(minv,min(val,maxv))
end

function vc2rc(vc)
  local x=0
  local y=0
  vc=flr(vc)
  if(vc==0) x=0 y=-1
  if(vc==1) x=1 y=-1
  if(vc==2) x=1 y=0
  if(vc==3) x=1 y=1
  if(vc==4) x=0 y=1
  if(vc==5) x=-1 y=1
  if(vc==6) x=-1 y=0
  if(vc==7) x=-1 y=-1
  return x,y
end

-- map
function mkmap()
  local mspr = 0
  for i=0,15 do
    for j=0,15 do
      if rnd(1)<den then
        mspr=swall
      else
        mspr=smpty
      end
      mset(i+mrx,j+mry,mspr)
    end
  end
  plcexit()
end

function getfreecoords()
  local x,y
  local gdmv=false
  while(not gdmv) do
    x=flr(rnd(15))
    y=flr(rnd(15))
    if(mget(mrx+x,mry+y)==0) gdmv=true
  end
  return x,y
end

function plcplyr()
  p.x,p.y=getfreecoords()  
end

function plcexit()
  local x,y=getfreecoords()
  mset(mrx+x,mry+y,sxit)
end

function plcrhino()
  r.x,r.y=getfreecoords()
end

-- game logic
function win()
  wins=wins+1
  sfx(4)
  swall=8+(wins%4)
  mkmap()
  reset()
end

function lose()
  losses=losses+1
  sfx(5)
  reset()
  attract=true
end

function reset()
  plcplyr()
  r.spr=rnorm
  plcrhino()
end

-- movement
function checkmv(x,y)
  local inbounds=x>=0 and x<=15 and y>=0 and y<=15
  local nspr=mget(mrx+x,mry+y)
  return inbounds and not fget(nspr,0)
end

function reachedexit()
  local nspr=mget(mrx+p.x,mry+p.y)
  return fget(nspr,3)
end

function playereaten()
  return (r.x==p.x and r.y==p.y)
end

function movep()
  local nx=p.x
  local ny=p.y
  if (btnp(0)) then nx=p.x-1 end
  if (btnp(1)) then nx=p.x+1 end
  if (btnp(2)) then ny=p.y-1 end
  if (btnp(3)) then ny=p.y+1 end
  nx=clamp(nx, 0, 15)
  ny=clamp(ny, 0, 15)

  if (checkmv(nx,ny) and (nx~=p.x or ny~=p.y)) then
    if (p.s) then
      sfx(0)
    else
      sfx(1)
    end
    p.x=nx
    p.y=ny
    p.s=not p.s
    mover()
  end
  if (reachedexit()) then
    win()
  end
end

function mover()
  if(r.chgvc==-1) then
    rnormalmv()
  else
    rchargemv()
  end
  if(playereaten()) lose()
end

function rnormalmv()
  local dx=p.x-r.x
  if(dx>0) then dx=dx/dx end
  if(dx<0) then dx=dx/-dx end
  local dy=p.y-r.y
  if(dy>0) then dy=dy/dy end
  if(dy<0) then dy=dy/-dy end
  -- add charge mv sel
  if(r.candmv and checkmv(r.x+dx,r.y+dy)) then
    r.x=r.x+dx
    r.y=r.y+dy
    r.spr=rnorm
    r.bored=0
  else
    if(dx~=0 and checkmv(r.x+dx,r.y)) then
      r.x=r.x+dx
      r.spr=rnorm
      r.bored=0
    else
      if(dy~=0 and checkmv(r.x,r.y+dy)) then
        r.y=r.y+dy
        r.spr=rnorm
        r.bored=0
      else
        if (r.canchg) r.bored=r.bored+1
        if(r.bored>(r.brlim*0.66)) then
          r.spr=rangry
        end
      end
    end
  end
  if (r.bored>r.brlim) then
    r.bored=0
    sfx(6)
    r.chschgvc()
    -- charge in random direction
  end
end

r.chschgvc=function()
  local gdmv=false
  local x,y
  while(not gdmv) do
    local vc=flr(rnd(8))
    x,y=vc2rc(vc)
    if(checkmv(r.x+x,r.y+y)) then
      gdmv=true
      r.chgvc=vc
    end
  end
  if(not gdmv) r.chgvc=-1
end

function rchargemv()
  local dx,dy
  if(r.chgdz==0) then
    r.chgvc=-1
    r.chgdz=2
  else
    r.chgdz=r.chgdz-1
    dx,dy=vc2rc(r.chgvc)
    if(checkmv(r.x+dx,r.y+dy)) then
      r.x=r.x+dx
      r.y=r.y+dy
    end
  end
end

-- pico8 callbacks
function _init()
  cls()
  mkmap()
  reset()
end

function _update()
  if(attract)then
    if(btn(4) or btn(5))then
      attract=false
      reset()
    end
  else
 		 if(r.chgvc==-1)then
   		 movep()
		  else
  		  mover()
		  end
		end
end

function _draw()
  cls()
  if(attract) then
    map(0,0,0,-12,16,16)
    print("z to start (easy)",28,108)
    print("x to start",28,114)
  else
    map(mrx,mry,0,0,16,16)
    if(p.s) then p.spr=1 else p.spr=2 end
  		spr(p.spr, p.x*8, p.y*8)
		  spr(r.spr,r.x*8,r.y*8)
  		print(r.bored..","..r.brlim..","..r.chgdz..":"..wins.."-"..losses,0,0,10)
  end
end
