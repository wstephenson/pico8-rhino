pico-8 cartridge // http://www.pico-8.com
version 4
__lua__
--globals
p={}
p.x=0
p.y=0
p.nx=p.x
p.ny=p.y
nspr=0
r={}
r.x=0
r.y=0
wins=0
losses=0
--map
mrx=16
mry=0
den=0.3

-- general
function clamp(val,minv,maxv)
  return max(minv,min(val,maxv))
end

-- map
function mkmap()
  mspr = 0
  for i=0,15 do
    for j=0,15 do
      if rnd(1)<den then
        mspr=2
      else
        mspr=0
      end
      mset(i+mrx,j+mry,mspr)
    end
  end
  plcexit()
end

function getfreecoords()
  good=false
  while(not good) do
    x=flr(rnd(15))
    y=flr(rnd(15))
    if(mget(mrx+x,mry+y)==0) good=true
  end
  return x,y
end

function plcplyr()
  p.x,p.y=getfreecoords()  
  p.nx=p.x
  p.ny=p.y
end

function plcexit()
  x,y=getfreecoords()
  mset(mrx+x,mry+y,4)
end

function plcrhino()
  r.x,r.y=getfreecoords()
end

-- game logic
function win()
  wins=wins+1
  mkmap()
  reset()
end

function lose()
  losses=losses+1
  reset()
end

function reset()
  plcplyr()
  plcrhino()
end

-- movement
function checkmv(x,y)
  nspr=mget(mrx+x,mry+y)
  return not fget(nspr, 0)
end

function movep()
	 if (btnp(0)) then p.nx=p.x-1 p.ny=p.y end
	 if (btnp(1)) then p.nx=p.x+1 p.ny=p.y end
	 if (btnp(2)) then p.ny=p.y-1 p.nx=p.x end
	 if (btnp(3)) then p.ny=p.y+1 p.nx=p.x end
  p.nx=clamp(p.nx, 0, 15)
  p.ny=clamp(p.ny, 0, 15)

  if (checkmv(p.nx,p.ny) and (p.nx~=p.x or p.ny~=p.y)) then
    p.x=p.nx
    p.y=p.ny
    mover()
  end
  if (fget(nspr,3)) then
    win()
  end
end  

function mover()
  dx=p.x-r.x
  if(dx>0) then dx=dx/dx end
  if(dx<0) then dx=dx/-dx end
  dy=p.y-r.y
  if(dy>0) then dy=dy/dy end
  if(dy<0) then dy=dy/-dy end
  if(checkmv(r.x+dx,r.y+dy)) then
    r.x=r.x+dx
    r.y=r.y+dy
    if(r.x==p.x and r.y==p.y) then lose() end
  end
end

-- pico8 callbacks
function _init()
  cls()
  mkmap()
  reset()
end

function _update()
  movep()
end

function _draw()
  cls()
  --background
  map(mrx,mry,0,0,16,16)
  --player
  spr(3, p.x*8, p.y*8)
  --rhino
  spr(1,r.x*8,r.y*8)
  --debug coords
  print(p.x..","..p.y.."/"..p.nx..","..p.ny..":"..wins.."-"..losses,0,0,10)
end
